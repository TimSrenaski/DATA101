  theme(plot.title = element_text(hjust = 0.5)) +
  theme(legend.position="none") +
  theme(text = element_text(size=6)) +
  theme(axis.text = element_text(size = 6)) +
  theme(axis.title = element_text(size = 8)) +
  ylim(0, 4075000)

