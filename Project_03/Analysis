NDDif <- NDDeg_25_39 /(NDDeg_25_39+NDDeg_40_64+NDDeg_65_plus)
OHDif <- OHDeg_25_39 /(OHDeg_25_39+OHDeg_40_64+OHDeg_65_plus)
