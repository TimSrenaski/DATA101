VADif <- VADeg_25_39 /(VADeg_25_39+VADeg_40_64+VADeg_65_plus)
WADif <- WADeg_25_39 /(WADeg_25_39+WADeg_40_64+WADeg_65_plus)
WVDif <- WVDeg_25_39 /(WVDeg_25_39+WVDeg_40_64+WVDeg_65_plus)
WIDif <- WIDeg_25_39 /(WIDeg_25_39+WIDeg_40_64+WIDeg_65_plus)
WYDif <- WYDeg_25_39 /(WYDeg_25_39+WYDeg_40_64+WYDeg_65_plus)


finaldf
lastdf <- rbind(finaldf, ALdf, AKdf, AZdf)
lastdf


